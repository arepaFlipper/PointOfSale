.Cart-on
  display flex
  justify-content flex-end
  align-items: center;
  background-color: #5f2e31
  display: flex
  height: 30px
  width: 100%
  position fixed
  bottom 50px
  &-checkout
    display flex
    justify-content space-evenly
    align-items center
    justify-items center
    width 30px
    margin 0 0 0 .5em
  &-alert
    color #33b13a
    font-size 14px
    margin-left 5px
  a
    text-decoration none
    color white
.Cart-off
  display none